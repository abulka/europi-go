  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run mock (3 lines)",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/mock"
    },
    {
      "name": "Run mock (4 lines) -lotslines",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/mock",
      "args": ["-lotslines"]
    },
    {
      "name": "Run mock (3 lines) -tea ",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/mock",
      "args": ["-tea"],
      "console": "integratedTerminal"
    },
    {
      "name": "Run mock (4 lines) -tea -lotslines",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/mock",
      "args": ["-tea", "-lotslines"],
      "console": "integratedTerminal"
    },
    {
      "name": "Scheduler Demo",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/scheduler-demo",
      "console": "integratedTerminal"
    },
  ]
}
